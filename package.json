        "@nestjs/cache-manager": "^2.2.2",
        "@nestjs/common": "^10.4.7",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.4.7",
        "@nestjs/event-emitter": "^2.1.1",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-fastify": "^10.4.4",
        "@nestjs/platform-socket.io": "^10.4.7",
