        "ts-loader": "^9.5.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "~5.5.4"
    },
    "pnpm": {
        "peerDependencyRules": {
            "allowedVersions": {}
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "moduleNameMapper": {
            "^~/(.*)$": "<rootDir>/$1"
        },
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "simple-git-hooks": {
        "pre-commit": "pnpm lint-staged"
    },
    "lint-staged": {
        "*": "eslint --fix"
    }
}
