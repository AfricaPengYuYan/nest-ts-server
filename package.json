        "@types/cache-manager": "^4.0.6",
        "@types/ioredis": "^5.0.0",
        "@types/jest": "29.5.14",
        "@types/lodash": "^4.17.13",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.9.0",
        "@types/passport-jwt": "^4.0.1",
        "@types/qs": "^6.9.17",
        "@types/request-ip": "^0.0.41",
        "@types/supertest": "^6.0.2",
        "@types/ua-parser-js": "^0.7.39",
        "cross-env": "^7.0.3",
        "eslint": "^9.11.1",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.10",
        "simple-git-hooks": "^2.11.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.4",
